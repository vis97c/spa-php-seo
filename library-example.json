{
    "_name": "APP NAME",
    "_title": "DEFAULT PAGE TITLE",
    "_description": "DEFAULT PAGE DESCRIPTION",
    "_image": "DEFAULT OG/TWITTER IMAGE",
    "_keywords": "DEFAULT CSV OF KEYWORDS",
    "_404": "/404",
    "pages": [
        {
            "name": "Home",
            "route": "/",
            "meta": {
                "title": "Seo | Home page"
            }
        },
        {
            "name": "Example",
            "route": "/example"
        },
        {
            "name": "Store",
            "route": "/store",
            "meta": {
                "title": "Store page",
                "image": "https://picsum.photos/id/237/200/300"
            }
        },
            {
                "name": "Fullpagination",
                "route": "/store/page/:pageNumber",
                "meta": {
                    "title": "Page $page$ : Seo | Store",
                    "image": "https://picsum.photos/id/237/200/300",
                    "_request": {
                        "url": "__HOST__/req.php",
                        "interger": [
                            ":pageNumber"
                        ],
                        "data": {
                            "page": ":pageNumber"
                        }
                    }
                }
            },
            {
                "name": "Search",
                "route": "/store/search",
                "query": {
                    "q": ":param"
                },
                "meta": {
                    "title": "Searching for \"$parameter$\" on Seo",
                    "image": "https://picsum.photos/id/237/200/300",
                    "_request": {
                        "url": "__HOST__/req.php",
                        "data": {
                            "parameter": ":param"
                        }
                    }
                }
            },
                {
                    "name": "Searchpagination",
                    "route": "/store/search/page/:pageNumber",
                    "query": {
                        "q": ":param"
                    },
                    "meta": {
                        "title": "Page $page$ : Searching for \"$parameter$\" on Seo",
                        "image": "https://picsum.photos/id/237/200/300",
                        "_request": {
                                "url": "__HOST__/req.php",
                                "interger": [
                                    ":pageNumber"
                                ],
                                "data": {
                                    "page": ":pageNumber",
                                    "parameter": ":param"
                                }
                            }
                        }
                    },
            {
                "name": "Category",
                "route": "/store/:category",
                "reserved": [
                    "search"
                ],
                "meta": {
                    "title": "$category$ on Seo",
                    "image": "https://picsum.photos/id/237/200/300",
                    "_request": {
                        "url": "__HOST__/req.php",
                        "data": {
                            "category": ":category"
                        }
                    }
                }
            },
                {
                    "name": "Categorypagination",
                    "route": "/store/:category/page/:pageNumber",
                    "reserved": [
                        "search"
                    ],
                    "meta": {
                        "title": "Page $page$ : $category$ on Seo",
                        "image": "https://picsum.photos/id/237/200/300",
                        "_request": {
                            "url": "__HOST__/req.php",
                            "interger": [
                                ":pageNumber"
                            ],
                            "data": {
                                "page": ":pageNumber",
                                "category": ":category"
                            }
                        }
                    }
                },
                {
                    "name": "ProductModal",
                    "route": "/store/:category/:productId",
                    "reserved": [
                        "search"
                    ],
                    "meta": {
                        "title": "Buy $nombre$ on Seo",
                        "image": "__THUMBS__/$foto$",
                        "_request": {
                            "url": "__API__/quickView",
                            "post": true,
                            "interger": [
                                ":productId"
                            ],
                            "data": {
                                "id": ":productId"
                            }
                        }
                    }
                },
        {
            "name": "Cart",
            "route": "/cart",
            "meta": {
                "title": "Seo | Your cart"
            }
        },
        {
            "name": "Support",
            "route": "/support",
            "meta": {
                "title": "Seo | Support"
            }
        },
        {
            "name": "error404",
            "route": "/404",
            "meta": {
                "title": "Seo | 404",
                "description": "Â¡Opps! Everything fall apart.",
                "image": "https://picsum.photos/seed/404/200/300"
            }
        }
    ]
}